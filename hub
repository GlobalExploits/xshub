local OrionLib = loadstring(game:HttpGet("https://pastebin.com/raw/6a4F5ENH", true))()

local Window = OrionLib:MakeWindow({Name = "XS Hub V3", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- Função para adicionar uma seção com o Discord
local function addDiscordSection(tab)
    return tab:AddSection({
        Name = "XS Hub - discord.gg/4cbym2UH5w"
    })
end

-- Tab Universal
local UniversalTab = Window:MakeTab({
    Name = "Universal",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

addDiscordSection(UniversalTab)

-- Botão Infinite Yield
UniversalTab:AddButton({
    Name = "Infinite Yield",
    Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source', true))()
    end
})

UniversalTab:AddButton({
    Name = "[FE] Remover ban Voice Chat",
    Callback = function()
        game:GetService("VoiceChatService"):joinVoice()
    end
})

-- Tab Player
local PlayerTab = Window:MakeTab({
    Name = "Player",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

addDiscordSection(PlayerTab)

-- Slider para Walkspeed
PlayerTab:AddSlider({
    Name = "Walkspeed",
    Min = 16,
    Max = 200,
    Default = 16,
    Color = Color3.fromRGB(255, 0, 0),
    Increment = 1,
    ValueName = "Walkspeed",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

-- Slider para JumpPower
PlayerTab:AddSlider({
    Name = "JumpPower",
    Min = 50,
    Max = 200,
    Default = 50,
    Color = Color3.fromRGB(255, 0, 0),
    Increment = 1,
    ValueName = "JumpPower",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end
})

-- Tab Dead Rails
